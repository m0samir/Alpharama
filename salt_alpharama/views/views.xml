<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id='view_stock_landed_cost_form' model='ir.ui.view'>
        <field name="name">stock.landed.cost.form</field>
        <field name="model">stock.landed.cost</field>
        <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>
        <field name="arch" type="xml">

            <field name="picking_ids" position="after">
                <field name="internal_transfer" options="{'no_quick_create':True,'no_create_edit':True, 'no_open': True,
                                        'no_show':True, 'no_create':True,}"/>
                <button string="Get Cost Lines" name="compute_internal_transfer" attrs="{'invisible':[('state','!=','draft')]}" type="object" class="oe_highlight"/>
            </field>

        </field>
    </record>

</odoo>
